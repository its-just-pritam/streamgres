							"const bodyArray = [];\r",
							"\r",
							"for (let i = 0; i < 10; i++) {\r",
							"    const randomName = \"Hello \" + Math.floor(Math.random() * 10);\r",
							"    const currentTimestamp = Math.floor(Date.now() / 1000);\r",
							"\r",
							"    bodyArray.push({\r",
							"        messageKey: randomName,\r",
							"        message: randomName,\r",
							"        timestamp: currentTimestamp\r",
							"    });\r",
							"}\r",
							"\r",
							"pm.variables.set(\"dynamicBody\", JSON.stringify(bodyArray));\r",
							""
						],
